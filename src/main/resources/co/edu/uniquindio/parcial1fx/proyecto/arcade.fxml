<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox prefHeight="591.0" prefWidth="684.0" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.parcial1fx.proyecto.ViewController.MainController">
    <AnchorPane maxHeight="1.0" maxWidth="1.0" prefHeight="1.0" prefWidth="1.0" VBox.vgrow="ALWAYS">
        <TabPane prefHeight="591.0" prefWidth="684.0" tabClosingPolicy="UNAVAILABLE">
            <Tab text="Usuario">
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="508.0" prefWidth="640.0">
                    <ImageView fx:id="imageViewLogo" fitHeight="115.0" fitWidth="130.0" layoutX="284.0"
                               layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="Imagenes/LogoProductivityPalace.jpg"/>
                    </ImageView>
                    <GridPane layoutX="242.0" layoutY="139.0" prefHeight="122.0" prefWidth="200.0">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        </rowConstraints>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nombre:"/>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Correo:"
                              GridPane.rowIndex="1"/>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Saldo:"
                              GridPane.rowIndex="2"/>
                        <TextField fx:id="txtNombre" GridPane.columnIndex="1"/>
                        <TextField fx:id="txtCorreo" GridPane.columnIndex="1"
                                   GridPane.rowIndex="1"/>
                        <TextField fx:id="txtSaldo" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                    </GridPane>
                    <Button layoutX="319.0" layoutY="281.0" mnemonicParsing="false"
                            onAction="#addUsuario" text="Crear"/>
                    <TableView fx:id="tableViewVehiculoTransporte" layoutY="332.0" prefHeight="172.0"
                               prefWidth="684.0">
                        <columns>
                            <TableColumn fx:id="tbNombre" prefWidth="217.0" text="Nombre"/>
                            <TableColumn fx:id="tbCorreo" minWidth="0.0" prefWidth="240.0"
                                         text="Correo"/>
                            <TableColumn fx:id="tbSaldo" prefWidth="226.0" text="Saldo"/>
                        </columns>
                    </TableView>
                    <Button layoutX="533.0" layoutY="516.0" mnemonicParsing="false"
                            onAction="#removeUsuario" text="Eliminar"/>
                    <Button layoutX="602.0" layoutY="516.0" mnemonicParsing="false"
                            onAction="#UpdateUsuario" text="Actualizar"/>
                </AnchorPane>
            </Tab>
            <Tab text="Usuarios Registrados">
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="416.0" prefWidth="640.0"/>
            </Tab>
        </TabPane>
    </AnchorPane>
</VBox>
